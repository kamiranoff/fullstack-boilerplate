{
  "name": "fullstack-boilerplate",
  "description": "A boilerplate application for express mongoose",
  "version": "0.0.1",
  "author": {
    "name": "Amiranoff Kevin",
    "email": "kamiranoff@gmail.com"
  },
  "engines": {
    "node": "0.12.x"
  },
  "scripts": {
    "start": "NODE_PATH=./server:./app NODE_ENV=development ./node_modules/.bin/nodemon server.js",
    "test": "make test"
  },
  "dependencies": {
    "async": "1.3.0",
    "body-parser": "1.13.2",
    "compression": "1.5.1",
    "connect-flash": "0.1.1",
    "connect-mongo": "0.8.1",
    "cookie-parser": "1.3.5",
    "cookie-session": "1.2.0",
    "csurf": "1.8.3",
    "express": "4.13.1",
    "express-session": "1.11.3",
    "forever": "0.14.2",
    "jade": "^1.11.0",
    "method-override": "2.3.3",
    "mongoose": "4.0.6",
    "mongoose-user": "latest",
    "morgan": "1.6.1",
    "passport": "0.2.2",
    "passport-local": "1.0.0",
    "view-helpers": "0.1.5",
    "winston": "1.0.1",
    "jquery": "~2.1.3",
    "normalize.css": "~3.0.1"
  },
  "devDependencies": {
    "babelify": "~6.0.2",
    "babel-core": "~5.6.15",
    "babel-eslint": "~4.0.0",
    "lodash": "~3.2.0",
    "foldero": "~0.1.1",
    "minimist": "~1.1.1",
    "rimraf": "~2.2.0",
    "wrench": "~1.5.8",
    "gulp-plumber": "~1.0.1",
    "gulp-changed": "~1.2.1",
    "gulp-rename": "~1.2.2",
    "gulp-uglify": "~1.2.0",
    "del": "~1.2.0",
    "gulp-if": "~1.2.5",
    "glob": "~5.0.10",
    "gulp-data": "~1.2.0",
    "gulp-sourcemaps": "~1.5.2",
    "gulp-load-plugins": "~0.10.0",
    "vinyl-source-stream": "~1.1.0",
    "vinyl-buffer": "~1.0.0",
    "gulp-util": "~3.0.5",
    "gulp": "~3.9.0",
    "browser-sync": "~2.7.12",
    "gulp-jade": "~1.0.1",
    "gulp-postcss": "~5.1.8",
    "autoprefixer-core": "~5.2.0",
    "gulp-htmlmin": "~1.1.3",
    "envify": "~3.4.0",
    "gulp-imagemin": "~2.2.1",
    "imagemin-svgo": "~4.0.0",
    "imagemin-pngquant": "~4.0.0",
    "gulp-eslint": "~0.14.0",
    "gulp-sass": "~2.0.1",
    "gulp-newer": "~0.5.1",
    "watchify": "~3.3.0 ",
    "browserify": "~10.0.0",
    "karma": "~0.12.21",
    "karma-browserify": "~4.2.1",
    "karma-phantomjs-launcher": "~0.1.2",
    "karma-chai": "~0.1.0",
    "karma-mocha": "~0.1.3",
    "nodemon": "latest",
    "mocha": "latest",
    "should": "latest",
    "supertest": "latest",
    "chai": "*"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kamiranoff/fullstack-boilerplate.git"
  },
   "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Local Server Settings",
    "host": "127.0.0.1",
    "port": "3002",
    "baseUrl": "./",
    "//": "Gulp Task Directories",
    "//": "NOTE: folders prefixed with an underscore (_) will have it removed when moved to build target (ex: src/_images -> build/images)",
    "//": "NOTE: folders NOT prefixed with underscore (_) will be copied to build target 1 to 1 (ex: src/fonts -> build/fonts)",
    "directories": {
      "source": "app",
      "destination": "build",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_images",
      "styles": "_styles",
      "scripts": "_scripts",
      "data": "_data"
    },
    "//": "Entry files",
    "entries": {
      "js": "main**.js",
      "css": "main**.{sass,scss}"
    }
  },
  "keywords": [
    "mvc",
    "boilerplate"
  ]
}
